{
  "name": "maham-monorepo",
  "private": true,
  "version": "0.1.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "npm@11.0.0",
  "engines": {
    "node": ">=18.17.0 || >=20.0.0"
  },
  "scripts": {
    "dev": "turbo run dev --parallel --filter=./apps/*",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "clean": "turbo run clean && git clean -fdX"
  },
  "devDependencies": {
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "turbo": "^2.5.6",
    "typescript": "^5.5.4"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@headlessui/react": "^2.2.7",
    "lucide-react": "^0.542.0",
    "react-countup": "^6.5.3",
    "rizzui": "^1.0.1"
  }
}
